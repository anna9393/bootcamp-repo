<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Cocktails</title>
    <link rel="stylesheet" type="text/css" href="/static/style.css" />
</head>

<body>
    <h1>Cocktails</h1>
    <ul>
        <a>ALL</a>
        <% list.forEach(e=>{ %>
            <a>
                <%= e.toUpperCase() %>
            </a>
            <% }); %>
    </ul>

    <ul>
        <% drinks.forEach(e=>{ %>
            <li>
                <%= e %>
            </li>
            <% }); %>
    </ul>
</body>

</html>